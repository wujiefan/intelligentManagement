.header{
  width: 750rpx;
  height: 321rpx;
  position: relative;
  padding: 30rpx;
  box-sizing: border-box;
  display: flex;
  background: url('data:image/png;base64,') 0 0/contain no-repeat;
}
.user-info{
  font-size:32rpx;
  font-weight:400;
  color: #FFF;
  margin-left:20rpx;
}
.user-name{
  font-size: 28rpx;
  margin-top: 20rpx;
}
.header open-data{
  display: inline-block;
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  overflow: hidden;
}

.middle{
  width: 750rpx;
  height: 181rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  position: relative;
}
.business-data{
  width: 100%;
  height: 181rpx;
  box-shadow:0px 3px 10px rgba(85,129,254,0.1);
  border-radius:14rpx;
  display: flex;
  margin-top: -40rpx;
  background: #FFF;
}
.business-data-item{
  height: 100%;
  flex: 1;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.business-data-item>view:first-child{
  font-size:36rpx;
  font-weight:bold;
}
.business-data-item>view:nth-child(2){
  font-size:26rpx;
}

.bottom{
  width: 750rpx;
  padding: 20rpx;
  box-sizing: border-box;
}
.nav-list{
  width: 100%;
  box-shadow:0px 3px 10px rgba(85,129,254,0.1);
  border-radius:14rpx;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  flex-wrap: wrap;
}
.nav-item{
  height: 221rpx;
  overflow: hidden;
  flex: 0 0 33%;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-right: 1px solid #F4F5FA;
  border-bottom: 1px solid #F4F5FA;

}
.nav-item image{
  width: 90rpx;
  height: 90rpx;
  margin-bottom: 27rpx;
}